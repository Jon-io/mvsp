{
  "name": "mvsp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf _site dist && yarn run eleventy && yarn run parcel build '_site/**/*.html'",
    "11ty:watch": "eleventy --watch",
    "parcel:watch": "parcel watch '_site/**/*.html'",
    "serve": "sirv dist --dev",
    "dev": "run-p -l 11ty:watch parcel:watch serve",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "yarn build && yarn cdk deploy \"MvspDev*\""
  },
  "devDependencies": {
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.0",
    "sirv-cli": "1.0.12"
  },
  "dependencies": {
    "@11ty/eleventy": "0.12.1",
    "@parcel/packager-raw-url": "2.0.0",
    "@parcel/transformer-webmanifest": "2.0.0",
    "@tailwindcss/typography": "0.4.1",
    "asciidoctor": "2.2.4",
    "autoprefixer": "10.2.5",
    "npm-run-all": "4.1.5",
    "parcel": "2.0.0",
    "postcss": "8.2.15",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.5",
    "tailwindcss": "2.1.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
